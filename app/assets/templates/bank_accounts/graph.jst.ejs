<form class="well form-search">
  <label>From Date:</label>
  <div class="input-append date" id="dp3" data-date="<%= fromDate %>" data-date-format="<%= App.util.localDateFormat %>">
    <input class="input-small" size="10" type="text" value="<%= fromDate %>" readonly="readonly">
    <span class="add-on"><i class="icon-th"></i></span>
  </div>
  <label>To Date:</label>
  <div class="input-append date" id="dp4" data-date="<%= toDate %>" data-date-format="<%= App.util.localDateFormat %>">
    <input class="input-small" size="10" type="text" value="<%= toDate %>" readonly="readonly">
    <span class="add-on"><i class="icon-th"></i></span>
  </div>
  <button type="submit" class="btn">Update</button>
</form>

