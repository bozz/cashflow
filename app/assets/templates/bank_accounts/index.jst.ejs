<h1>Banking</h1>

<button class="btn-new btn btn-primary" type="button">New Bank Account</button>

<table class="table table-striped">
  <thead>
    <tr>
      <th>id</th>
      <th>Account Name</th>
      <th>Bank</th>
      <th>Account Number</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
<% for(var i=0; i<accounts.length; i++) {
    var account = accounts.models[i];
%>
    <tr>
      <td><%= account.get('id') %></td>
      <td><a href="/#banking/<%= account.get('id') %>"><%= account.get('name') %></a></td>
      <td><%= account.get('bank') %></td>
      <td><%= account.get('account_number') %></td>
      <td>
        <!-- <button class="btn-edit btn btn-mini" type="button" value="<%= account.get('id') %>">Edit</button> -->
        <button class="btn-delete btn btn-danger btn-mini" type="button" value="<%= account.get('id') %>">Delete</button>
      </td>
    </tr>
<% } %>
  </tbody>
</table>
